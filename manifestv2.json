{
   "author": "Alexander N & ShaggyZE",
   "background": {
   "scripts": ["jquery.js", "background.js"]
   },
   "content_scripts": [ {
      "css": [ "style.css", "gitgraph.css" ],
      "exclude_matches": [ "*://myanimelist.net/login*", "*://myanimelist.net/register*", "*://myanimelist.net/editprofile.php?go=myoptions" ],
      "js": [ "malench.js", "bbcodehelper.js", "bbcodeparser.js" ],
      "matches": [ "https://myanimelist.net/*" ]
   } ],
   "description": "Enhances MAL for a better user experience. Suggestions are welcome!",
   "icons": {
      "128": "logo128.png",
      "16": "logo16.png",
      "48": "logo48.png"
   },
   "manifest_version": 2,
   "name": "MAL Enhancer",
   "options_ui": {
   "page": "options.html"
   },
   "permissions": [ "storage" ],
   "short_name": "MALENCH",
   "browser_specific_settings": {
   "gecko": {
   "id": "shaggyze@gmail.com",
   "strict_min_version": "63.0"
   }
   },
   "version": "1.6",
   "web_accessible_resources": [ "emoji/*.json", "*.png", "style.css", "bbcodehelper.html", "animeRandomizer.html" ]
}
